{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"prisma:generate": {
			"cache": false
		},
		"prisma:db:pull": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"outputs": ["dist/**", ".next/**"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	}
}
